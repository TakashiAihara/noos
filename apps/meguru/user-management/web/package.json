{
  "name": "@meguru/user-web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "clean": "git clean -dfx dist .next .turbo node_modules",
    "dev": "next dev --turbo",
    "start": "next build && next start",
    "start:dev": "pnpm dev",
    "start:prod": "pnpm build && pnpm start",
    "test": "pnpm test:base",
    "test:all": "pnpm test:unit && pnpm test:integration",
    "test:base": "vitest",
    "test:benchmark": "pnpm test:base --mode benchmark",
    "test:cov": "pnpm test:base --coverage",
    "test:debug": "pnpm test:base --inspect-brk --no-file-parallelism",
    "test:debug:single-fork": "pnpm test:base --inspect-brk --pool forks --poolOptions.forks.singleFork",
    "test:debug:single-thread": "pnpm test:base --inspect-brk --pool threads --poolOptions.threads.singleThread",
    "test:integration": "pnpm test:base -c vitest.config.integration.ts",
    "test:snapshot:update": "pnpm test:base --update --ui",
    "test:unit": "pnpm test:base",
    "test:watch": "pnpm test:base --watch",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@meguru/db": "workspace:",
    "@meguru/env": "workspace:",
    "@meguru/shared-constants": "workspace:^",
    "@meguru/shared-schemas": "workspace:",
    "@meguru/shared-utils": "workspace:",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.71.10",
    "@trpc/client": "^11.0.2",
    "@trpc/react-query": "^11.0.2",
    "@trpc/server": "^11.0.2",
    "axios": "^1.8.4",
    "geist": "^1.3.1",
    "next": "^15.2.4",
    "next-auth": "4.24.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "@tooling/tsconfig": "workspace:",
    "@tsconfig/strictest": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitest/coverage-v8": "catalog:",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3",
    "tsc-alias": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=22.14.0",
    "npm": "DO NOT USE NPM",
    "pnpm": "10.15.0",
    "yarn": "DO NOT USE YARN"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  }
}
